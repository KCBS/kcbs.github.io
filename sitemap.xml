<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for post in site.posts %}
    <url>
        <loc>http://kcbs.github.io{{ post.url | remove: 'index.html' }}</loc>
    </url>
    {% endfor %}

    {% for page in site.pages %}
    <url>
        <loc>http://kcbs.github.io{{ page.url | remove: 'index.html' }}</loc>
    </url>
    {% endfor %}
</urlset>
